<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1"> <!--To make the sequence not changing if updating the module-->

        <record id="seq_regis_client" model="ir.sequence">
            <field name="name">Clients Sequence</field> <!--Name of sequence-->
            <field name="code">regis.client</field> <!--Name of model that you want to sequence-->
            <field name="prefix">C</field> <!--Prefix of sequence-->
            <field name="padding">5</field> <!--Padding of sequence-->
            <field name="number_increment">1</field> <!--Number increment of sequence-->
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>